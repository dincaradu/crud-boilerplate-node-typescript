{
  "name": "crud-boilerplate",
  "version": "1.0.0",
  "description": "This a CRUD boilerplate written in Typescript with Node.js, Express, Sequelize, MySQL.",
  "main": "src/dist/index.js",
  "scripts": {
    "clean": "rimraf src/dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc copy-assets",
    "build:prod": "set NODE_ENV=production && npm run build",
    "dev:start": "npm-run-all build start",
    "dev": "set NODE_ENV=development && nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dincaradu/crud-boilerplate-node-js-express-sequelize.git"
  },
  "keywords": [
    "Node.js",
    "CRUD",
    "boilerplate",
    "Express",
    "Sequelize",
    "MySQL",
    "Authentication",
    "JWT"
  ],
  "author": "Radu DincÄƒ",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dincaradu/crud-boilerplate-node-js-express-sequelize/issues"
  },
  "homepage": "https://github.com/dincaradu/crud-boilerplate-node-js-express-sequelize#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^14.17.5",
    "@types/shelljs": "^0.8.10",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  }
}
